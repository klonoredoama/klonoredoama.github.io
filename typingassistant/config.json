  "accept_key": "insert",
  "max_buffer": 40,

  "replacement": {
    "pre_backspace_delay": 0.01,
    "post_backspace_delay": 0.01,
    "per_character_delay": 0.0
  },

  "logging": {
    "enabled": true,
    "filename": "app.log"
  },

  "startup": {
    "print_banner": true,
    "print_dictionary_count": true
  }
}

